{
  "extends": "airbnb",
  "rules": {
    "no-param-reassign": "off",
    "no-shadow": 0,
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": ["off"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  }
}
